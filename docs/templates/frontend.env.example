# Frontend Environment Variables Template
# Copy this file to app/frontend/.env.local and fill in your actual values
# NEVER commit .env or .env.local files to version control

# API Configuration
VITE_API_URL=http://localhost:9000
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Crypto Curriculum Platform
VITE_APP_DESCRIPTION=Blockchain & Cryptocurrency Education Platform

# Feature Flags (optional)
VITE_ENABLE_TRADING_BOT=true
VITE_ENABLE_ANALYTICS=false

# External Services (optional - for advanced features)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_SENTRY_DSN=https://...

